{
  "name": "bpmn-visualization",
  "version": "0.43.0-post",
  "type": "module",
  "sideEffects": false,
  "description": "A TypeScript library for visualizing process execution data on BPMN diagrams",
  "keywords": [
    "typescript",
    "bpmn",
    "visualization",
    "process",
    "analytics",
    "browser"
  ],
  "homepage": "https://github.com/process-analytics/bpmn-visualization-js#readme",
  "bugs": "https://github.com/process-analytics/bpmn-visualization-js/issues",
  "license": "Apache-2.0",
  "contributors": [
    {
      "name": "Thomas Bouffard",
      "url": "https://github.com/tbouffard"
    },
    {
      "name": "CÃ©line Souchet",
      "url": "https://github.com/csouchet"
    },
    {
      "name": "Marcin Michniewicz",
      "url": "https://github.com/aibcmars"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/process-analytics/bpmn-visualization-js.git"
  },
  "browser": "dist/bpmn-visualization.esm.js",
  "module": "dist/bpmn-visualization.esm.js",
  "types": "dist/not-supported-ts-versions.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/bpmn-visualization.d.ts",
        "default": "./dist/bpmn-visualization.esm.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    ">=4.0.2": {
      "*": [
        "dist/bpmn-visualization.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "NOTICE"
  ],
  "scripts": {
    "dev": "bunx --bun vite",
    "dev-bun": "bun --watch run ./dev/ts/pages/index.ts",
    "all": "run-s clean lint lint-check build test",
    "clean": "rimraf build dist",
    "build": "tsc --noEmit",
    "build-bundles": "rollup --strictDeprecations -c",
    "build-dir": "bun build --minify --entrypoints ./src/bpmn-visualization.ts --sourcemap=external --outdir ./dist/bun/dir",
    "build-esm": "bun build --entrypoints ./src/bpmn-visualization.ts --outdir ./dist/bun/esm --format esm --entry-naming [dir]/[name].esm.[ext]",
    "build-iife": "bun build --minify --entrypoints ./src/bpmn-visualization.ts --outdir ./dist/bun/min --format iife --entry-naming [dir]/[name].min.[ext]",
    "generate-types": "run-s generate-types:*",
    "generate-types:lib": "tsc --project tsconfig.npm-package.json --emitDeclarationOnly --declaration --declarationDir build/types",
    "generate-types:dist": "api-extractor run --local --verbose",
    "generate-types:not-supported-ts-versions": "bun run scripts/generate-types-for-not-supported-ts-versions.mjs",
    "generate-types:add-license-header": "bun run scripts/add-license-header.mjs",
    "prepack": "run-s clean generate-types build-bundles",
    "demo": "run-s demo:*",
    "demo:build": "tsc --noEmit && vite build",
    "demo:prepare": "bun run scripts/prepare-demo-for-publish.mjs",
    "demo-preview": "vite preview",
    "docs": "run-s docs:*",
    "docs:user": "bun run scripts/generate-users-documentation.mjs",
    "docs:api": "typedoc --tsconfig ./tsconfig.typedoc.json src/bpmn-visualization.ts",
    "lint": "eslint \"**/*.{js,cjs,mjs,ts,cts,mts}\" --max-warnings 0 --quiet --fix",
    "lint-check": "eslint \"**/*.{js,cjs,mjs,ts,cts,mts}\" --max-warnings 0",
    "test": "run-s test:unit test:integration test:e2e",
    "test:unit": "jest --runInBand --config=./test/unit/jest.config.cjs",
    "test:unit:coverage": "bun run test:unit -- --coverage",
    "test:integration": "jest --runInBand --config=./test/integration/jest.config.cjs",
    "test:integration:coverage": "bun run test:integration -- --coverage",
    "test:unit:watch": "bun run test:unit:coverage -- --watchAll",
    "test:e2e": "cross-env JEST_IMAGE_SNAPSHOT_TRACK_OBSOLETE=1 jest --runInBand --detectOpenHandles --config=./test/e2e/jest.config.cjs",
    "test:e2e:verbose": "cross-env DEBUG=bv:test:*,pw:browser* bun run test:e2e",
    "test:e2e:coverage": "bun run test:e2e -- --coverage",
    "test:perf": "jest --runInBand --detectOpenHandles --config=./test/performance/jest.config.cjs",
    "test:perf:verbose": "cross-env DEBUG=bv:test:*,pw:browser* bun run test:perf",
    "test:perf:compile": "tsc -p test/performance/",
    "test:bundles": "jest --runInBand --detectOpenHandles --config=./test/bundles/jest.config.cjs",
    "test:bundles:verbose": "cross-env DEBUG=bv:*,pw:browser* bun run test:bundles",
    "version-prepare": "bun run scripts/manage-version-in-files.mjs && git commit --no-verify -a -m \"chore(release): prepare version for release\"",
    "version": "IS_RELEASING=true bun run scripts/manage-version-in-files.mjs && git add .",
    "postversion": "bun run scripts/manage-version-in-files.mjs && git commit --no-verify -a -m \"chore(release): prepare version for new developments\"",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "prepare": "run-s prepare:*",
    "prepare:husky": "husky",
    "utils-all": "run-s utils:build utils:test:*",
    "utils:build": "rollup --strictDeprecations --config scripts/utils/rollup.config.mjs",
    "utils:test:json": "bun run ./scripts/utils/dist/utils.mjs test/fixtures/bpmn/simple-start-task-end.bpmn --output json",
    "utils:test:model": "bun run ./scripts/utils/dist/utils.mjs test/fixtures/bpmn/simple-start-task-end.bpmn --output model"
  },
  "dependencies": {
    "@typed-mxgraph/typed-mxgraph": "~1.0.8",
    "fast-xml-parser": "4.3.4",
    "lodash-es": "~4.17.21",
    "mxgraph": "4.2.2",
    "strnum": "1.0.5"
  },
  "devDependencies": {
    "@asciidoctor/core": "~3.0.3",
    "@microsoft/api-extractor": "~7.39.4",
    "@rollup/plugin-commonjs": "~25.0.7",
    "@rollup/plugin-node-resolve": "~15.2.3",
    "@rollup/plugin-terser": "~0.4.4",
    "@types/bun": "^1.0.12",
    "@types/debug": "~4.1.12",
    "@types/jest-image-snapshot": "~6.4.0",
    "@types/lodash-es": "~4.17.12",
    "@types/minimist": "~1.2.5",
    "@typescript-eslint/eslint-plugin": "~7.5.0",
    "@typescript-eslint/parser": "~7.5.0",
    "autoprefixer": "~10.4.19",
    "clipboardy": "~4.0.0",
    "cross-env": "~7.0.3",
    "cssnano": "~6.1.2",
    "debug": "~4.3.4",
    "eslint": "~8.57.0",
    "eslint-config-prettier": "~9.1.0",
    "eslint-import-resolver-typescript": "~3.6.1",
    "eslint-plugin-import": "~2.29.1",
    "eslint-plugin-jest": "~27.9.0",
    "eslint-plugin-jest-extended": "^2.0.0",
    "eslint-plugin-notice": "~0.9.10",
    "eslint-plugin-playwright": "~1.5.4",
    "eslint-plugin-prettier": "~5.1.3",
    "eslint-plugin-unicorn": "~51.0.1",
    "expect-playwright": "~0.8.0",
    "fs-extra": "~11.2.0",
    "husky": "~9.0.11",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "~29.7.0",
    "jest-extended": "~4.0.2",
    "jest-html-reporter": "~3.10.2",
    "jest-html-reporters": "~3.1.7",
    "jest-image-snapshot": "~6.4.0",
    "jest-playwright-preset": "~4.0.0",
    "jest-sonar": "~0.2.16",
    "lint-staged": "~15.2.2",
    "minimist": "~1.2.8",
    "npm-run-all": "~4.1.5",
    "pinst": "~3.0.0",
    "playwright": "~1.41.2",
    "postcss": "~8.4.38",
    "postcss-cli": "~11.0.0",
    "prettier": "~3.2.5",
    "rimraf": "~5.0.5",
    "rollup": "~4.13.2",
    "rollup-plugin-cleanup": "~3.2.1",
    "rollup-plugin-node-externals": "~7.1.1",
    "rollup-plugin-sizes": "~1.0.6",
    "rollup-plugin-typescript2": "~0.36.0",
    "tailwindcss": "~3.4.3",
    "ts-jest": "~29.1.2",
    "typedoc": "~0.25.12",
    "typescript": "~5.4.3",
    "vite": "~5.0.12"
  },
  "overrides": {
    "@types/node": "^16.18.0"
  },
  "lint-staged": {
    "*.{js,cjs,mjs,ts,cts,mts}": [
      "eslint --fix"
    ]
  }
}
